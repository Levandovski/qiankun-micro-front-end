  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main-react-app": "cd main-react-app && npm i",
    "install:micro-react-sub-app1": "cd micro-react-sub-app1 && npm i",
    "install:micro-react-sub-app2": "cd micro-react-sub-app2 && npm i",
    "install:micro-react-sub-app3-ts-vite": "cd micro-react-sub-app3-ts-vite && npm i",
    "install:micro-react-sub-app4-ts-cra": "cd micro-react-sub-app4-ts-cra && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:main-react-app": "cd main-react-app && npm start",
    "start:micro-react-sub-app1": "cd micro-react-sub-app1 && npm start",
    "start:micro-react-sub-app2": "cd micro-react-sub-app2 && npm start",
    "start:micro-react-sub-app3-ts-vite": "cd micro-react-sub-app3-ts-vite && npm run dev",
    "start:micro-react-sub-app4-ts-cra": "cd micro-react-sub-app4-ts-cra && npm start",
    "build": "npm-run-all build:*",
    "build:main-react-app": "cd main-react-app && npm run build",
    "build:micro-react-sub-app1": "cd micro-react-sub-app1 && npm run build",
    "build:micro-react-sub-app2": "cd micro-react-sub-app2 && npm run build",
    "build:micro-react-sub-app3-ts-vite": "cd micro-react-sub-app3-ts-vite && npm run build",
    "build:micro-react-sub-app4-ts-cra": "cd micro-react-sub-app4-ts-cra && npm run build"
